{
  "name": "@rwk/physics-math",
  "version": "0.1.35",
  "description": "Math for physics homework problems",
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "browser": "lib/umd/index.js",
  "jsdelivr": "lib/umd/index.js",
  "unpkg": "lib/umd/index.js",
  "types": "lib/esm/index.d.ts",
  "scripts": {
    "imports:linkrewriter": "cd imports/typedoc-plugin-linkrewriter && npm install && npm run build",
    "build:imports": "npm run imports:linkrewriter",
    "prebuild:config": "tsc --project config/tsconfig.json",
    "build:config": "",
    "prebuild:devtools": "npm run build:config",
    "build:devtools": "tsc --project devtools/tsconfig.json",
    "pretest": "tsc --project src/__tests__ && npm run build:config",
    "test": "jest",
    "prebuild": "npm run build:devtools && npm run build:imports",
    "build": "rollup -c",
    "postbuild": "npm run build:docs",
    "watch": "NODE_ENV=development rollup -c --watch",
    "clean": "rm -rf build lib docs",
    "prepublishOnly": "NODE_ENV=production npm run build && npm run pubdocs",
    "prebuild:docs": "test -e node_modules/typedoc-plugin-linkrewriter/dist/index.js || (npm run build:imports && npm install)",
    "build:docs": "GRAPHVIZ_DOT=\"$(which dot)\" typedoc --excludeNotExported --stripInternal --rewriteLinks build/config/link-rewriter.js --mode modules --out build/docs/api src/",
    "preserve": "npm run build:devtools",
    "serve": "NODE_ENV=development SERVE=true rollup -c",
    "predocs": "npm run build:config && npm run build:docs",
    "docs": "NODE_ENV=development SERVE=true SERVE_DOC=true rollup -c",
    "prewatch": "npm run build:devtools",
    "pubdocs": "bin/deploy-docs.js"
  },
  "bin": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BobKerns/physics-math.git"
  },
  "keywords": [
    "node",
    "browser",
    "observablehq",
    "physics",
    "education",
    "math",
    "typescript"
  ],
  "author": "Bob Kerns",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BobKerns/physics-math/issues"
  },
  "homepage": "https://github.com/BobKerns/physics-math#readme",
  "dependencies": {
    "@observablehq/runtime": "^4.8.0",
    "@observablehq/stdlib": "^3.4.0",
    "@types/d3": "^6.2.0",
    "d3": "^6.3.1",
    "genutils": "^0.1.38",
    "gl-matrix": "^3.3.0",
    "katex": "^0.12.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@types/jest": "^24.9.1",
    "@types/katex": "^0.11.0",
    "@types/ramda": "^0.26.44",
    "jest": "^26.6.3",
    "jest-mock-proxy": "^3.0.0",
    "mermaid": "^8.8.4",
    "ramda": "^0.26.1",
    "rollup": "^2.35.1",
    "rollup-plugin-external-globals": "^0.5.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-typescript2": "^0.21.2",
    "rollup-plugin-visualizer": "^1.1.1",
    "serve": "^11.3.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.17.8",
    "typedoc-plugin-external-module-name": "^3.1.0",
    "typedoc-plugin-linkrewriter": "file:imports/typedoc-plugin-linkrewriter",
    "typedoc-plugin-mermaid": "^1.3.0",
    "typedoc-plugin-npm-externals": "^1.0.0",
    "typescript": "^3.9.7",
    "wcsize": "^1.0.0"
  }
}
